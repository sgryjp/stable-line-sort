{
    "name": "stable-line-sort",
    "displayName": "Stable Line Sort",
    "description": "Sort Lines using stable sort algorithm.",
    "version": "0.1.0",
    "publisher": "sgryjp",
    "repository": {
        "type": "git",
        "url": "https://github.com/sgryjp/stable-line-sort.git"
    },
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.doStableLineSortAscending",
        "onCommand:extension.doStableLineSortDescending"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.doStableLineSortAscending",
                "title": "Sort Lines Ascending",
                "category": "Stable Line Sort"
            },
            {
                "command": "extension.doStableLineSortCustom",
                "title": "Sort Lines Descending",
                "category": "Stable Line Sort"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "ts-node": "^7.0.1",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    }
}
